{
  "name": "jobaas-backend",
  "version": "0.1.3",
  "description": "Backend server for Job as a service application",
  "main": "set NODE_ENV=dev && nodemon server.js",
  "engines": {
    "node": "14.19.2"
  },
  "scripts": {
    "sonar": "node sonar-project.js",
    "dev": "set NODE_ENV=dev && nodemon server.js",
    "release": "set NODE_ENV=release && node server.js",
    "testSemi": "set NO_MAIL=0 && mocha --timeout 30000 --exit test/0_welcome test/1_particular",
    "test": "set MODE_TEST=1 && set NO_MAIL=1 &&  set EXCEPTION_TRANSACTION=1 && mocha --timeout 30000 --exit test/0_welcome test/1_administrator test/1_particular test/1_particular_administrator test/2_job test/3_application test/4_transaction  test/6_evaluation test/7_notification test/8_company test/9_prospect test/11_experience  test/12_fileManager  test/13_aws_fileManager test/14_blogArticles test/15_collaborator",
    "cover": "nyc --reporter=text mocha",
    "test_migration": "set MODE_TEST=1 && set NO_MAIL=1 && mocha --timeout 30000 --exit test/0_welcome test/1_administrator test/1_particular test/1_particular_administrator test/8_company test/2_job test/3_application test/6_evaluation test/7_notification test/9_prospect test/10_contract",
    "start": "node server.js",
    "test:production": "set MODE_TEST=1 && set NO_MAIL=1 && set NODE_ENV=production && mocha --exit",
    "test:release": "set MODE_TEST=1 && set NO_MAIL=1 && set NODE_ENV=release && mocha --exit",
    "eslint": "eslint ./",
    "eslint-fix": "eslint ./ --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/jobaas/back/project.git"
  },
  "author": "Jobaas",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/jobaas/back/project/issues"
  },
  "homepage": "https://gitlab.com/jobaas/back/project/-/blob/master/README.md",
